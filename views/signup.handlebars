<form id="signup_form">
    <div class="mb-3">
        <label for="email" class="form-label">Email address</label>
        <input type="email" class="form-control" id="signup-email" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
        <label for="password" class="form-label">Password</label>
        <input type="password" class="form-control" id="signup-password">
    </div>
    <div class="mb-3">
        <label for="firstName" class="form-label">First Name</label>
        <input type="name" class="form-control" id="signup-fn">
    </div>
    <div class="mb-3">
        <label for="lastName" class="form-label">Last Name</label>
        <input type="name" class="form-control" id="signup-ln">
    </div>
     <div class="mb-3">
        <label for="phoneNumber" class="form-label">Phone Number</label>
        <input type="number" class="form-control" id="signup-pn">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

<script type="text/javascript" src="/public/js/signup.js"></script>